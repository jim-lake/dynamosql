{
  "name": "@dynamosql/dynamosql",
  "version": "0.0.1",
  "description": "MySQL compatible query engine with DynamoDB storage engine",
  "main": "dist/dynamosql.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "pretty": "prettier --write src test examples",
    "ts:check": "tsc --noEmit",
    "test:setup": "node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js test/setup/setup.js",
    "test": "node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js 'test/**/*.test.js'"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.922.0",
    "async": "3.2.4",
    "big-integer": "1.6.51",
    "sqlstring": "2.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.0",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "mysql": "2.18.1",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
