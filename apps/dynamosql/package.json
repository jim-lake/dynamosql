{
  "name": "@dynamosql/dynamosql",
  "version": "0.0.1",
  "description": "MySQL compatible query engine with DynamoDB storage engine",
  "main": "dist/dynamosql.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src test examples --fix",
    "lint:single": "eslint",
    "pretty": "prettier -u --write \"**/*\" --log-level warn",
    "ts:check": "tsc --noEmit",
    "test:export:check": "tsc --noEmit -p test/import_compare_tsconfig.json",
    "test:setup": "dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js test/setup/setup.js",
    "test:single": "dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js",
    "test:single:slow": "TEST_RUN_SLOW=1 dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js",
    "test": "dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js 'test/**/*.test.js' 'test/**/*.test.ts'",
    "test:coverage": "TEST_RUN_SLOW=1 dotenv -- c8 --reporter=html --reporter=text --exclude='test/**' --exclude='examples/**' --exclude='dist/**' node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js --no-clean --reporter test/silent_reporter.js 'test/**/*.test.js'",
    "test:quiet": "dotenv -- npx tsx --no-warnings ../../node_modules/.bin/mocha --no-clean --reporter test/silent_reporter.js 'test/**/*.test.js' 'test/**/*.test.ts'",
    "test:quiet:slow": "TEST_RUN_SLOW=1 dotenv -- npx tsx --no-warnings ../../node_modules/.bin/mocha --no-clean --reporter test/silent_reporter.js 'test/**/*.test.js' 'test/**/*.test.ts'",
    "query:mysql": "dotenv -- npx tsx test/test_client.js",
    "query:session": "dotenv -- npx tsx test/test_session.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.922.0",
    "node-sql-parser": "*",
    "sqlstring": "2.3.3"
  },
  "devDependencies": {
    "@dynamosql/lint": "*",
    "@types/mysql": "^2.15.27",
    "@types/sqlstring": "^2.3.2",
    "mysql": "^2.18.1"
  }
}
