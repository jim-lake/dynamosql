{
  "name": "@dynamosql/dynamosql",
  "version": "0.0.1",
  "description": "MySQL compatible query engine with DynamoDB storage engine",
  "main": "dist/dynamosql.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src test examples",
    "pretty": "prettier -u --write \"**/*\" --log-level warn",
    "ts:check": "tsc --noEmit",
    "test:export:check": "tsc --noEmit -p test/import_compare_tsconfig.json",
    "test:setup": "dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js test/setup/setup.js",
    "test": "dotenv -- node --require tsx/cjs ../../node_modules/mocha/bin/mocha.js 'test/**/*.test.js'",
    "test:quiet": "dotenv -- npx tsx --no-warnings ../../node_modules/.bin/mocha --no-clean --reporter test/silent_reporter.js 'test/**/*.test.js'"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.922.0",
    "big-integer": "^1.6.52",
    "sqlstring": "^2.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.10.0",
    "async": "^3.2.6",
    "chai": "^6.2.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.20.0",
    "mocha": "^11.7.5",
    "mysql": "^2.18.1",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.23.0"
  }
}
